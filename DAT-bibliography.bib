	author = {Farah, Ilijas and Solecki, Sławomir},
	urldate = {2013-11-04},
	date = {2006-01},
	keywords = {Borel complexity of subgroups, Densely divisible groups, Maximal divisible subgroups, Polish groups, Polishable subgroups},
}

