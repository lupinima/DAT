@article{kavruk_weak_2012,
	title = {The Weak Expectation Property and Riesz Interpolation},
	url = {http://arxiv.org/abs/1201.5414},
	abstract = {We show that Lance's weak expectation property is connected to tight Riesz interpolations in lattice theory. More precisely we first prove that if A {\textbackslash}textbackslashsubset B(H) is a unital C*-subalgebra, where B(H) is the bounded linear operators on a Hilbert space H, then A has (2,2) tight Riesz interpolation property in B(H) (defined below). An extension of this requires an additional assumption on A: A has (2,3) tight Riesz interpolation property in B(H) at every matricial level if and only if A has the weak expectation property. Let \$J = span\{(1,1,-1,-1,-1)\}\$ in \$Cˆ5\$ . We show that a unital C*-algebra A has the weak expectation property if and only if \$A {\textbackslash}textbackslashotimesmin (Cˆ5/J) = A {\textbackslash}textbackslashotimesmax (Cˆ5/J)\$ (here {\textbackslash}textbackslashotimesmin and {\textbackslash}textbackslashotimesmax are the minimal and the maximal operator system tensor products, respectively, and \$Cˆ5/J\$ is the operator system quotient of \$Cˆ5\$ by \$J\$). We express the Kirchberg conjecture ({KC}) in terms of a four dimensional operator system problem. We prove that {KC} has an affirmative answer if and only if \$Cˆ5/J\$ has the double commutant expectation property if and only if \$C5/J {\textbackslash}textbackslashotimesmin C5/J = C5/J {\textbackslash}textbackslashotimesc C5/J\$ (here {\textbackslash}textbackslashotimesc represents the commuting operator system tensor product).},
	journaltitle = {{arXiv}:1201.5414 [math]},
	author = {Kavruk, Ali S.},
	urldate = {2017-01-01},
	date = {2012-01},
}

