@article{giordano_zd-odometers_2017,
	title = {\$Zˆ\{d\}\$-odometers and cohomology},
	url = {http://arxiv.org/abs/1709.08585},
	abstract = {Cohomology for actions of free abelian groups on the Cantor set has (when endowed with an order structure) provided a complete invariance for orbit equivalence. In this paper, we study a particular class of actions of such groups called odometers (or profinite actions) and investigate their cohomology. We show that for a free, minimal \${\textbackslash}{textbackslashZˆ}\{d\}\$-odometer, the first cohomology group provides a complete invariant for the action up to conjugacy. This is in contrast with the situation for orbit equivalence where it is the cohomology in dimension \$d\$ which provides the invariant. We also consider classification up to isomorphism and continuous orbit equivalence.},
	journaltitle = {{arXiv}:1709.08585 [math]},
	author = {Giordano, Thierry and Putnam, Ian F. and {SKau}, Christian F.},
	urldate = {2019-05-28},
	date = {2017-09},
	keywords = {Mathematics - Dynamical Systems, 20J06, 37B99, 37C85},
}

@article{lewis_protori_2019,
	title = {Protori and Torsion-Free Abelian Groups},
	url = {http://arxiv.org/abs/1903.08022},
	abstract = {The Resolution Theorem for Compact Abelian Groups is applied to show that the profinite subgroups of a finite-dimensional compact connected abelian group (protorus) which induce tori quotients comprise a lattice under intersection (meet) and \$+\$ (join), facilitating a proof of the existence of a universal resolution. A finite rank torsion-free abelian group \$X\$ is algebraically isomorphic to a canonical dense subgroup \$X\_G\$ of its Pontryagin dual \$G\$. A morphism between protori lifts to a product morphism between the universal covers, so morphisms in the category can be studied as pairs of maps: homomorphisms between finitely generated profinite abelian groups and linear maps between finite-dimensional real vector spaces. A concept of non-Archimedean dimension is introduced which acts a useful invariant for classifying protori.},
	journaltitle = {{arXiv}:1903.08022 [math]},
	author = {Lewis, Wayne},
	urldate = {2019-05-28},
	date = {2019-03},
	keywords = {Mathematics - Group Theory, 20K15, 20K20, 20K25, 22B05, 22C05, 22D35},
}

