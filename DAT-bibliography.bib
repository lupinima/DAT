@article{hoff_von_2015,
	title = {Von Neumann Algebras of Equivalence Relations with Nontrivial One-Cohomology},
	url = {http://arxiv.org/abs/1506.00588},
	abstract = {Using Popa's deformation/rigidity theory, we investigate prime decompositions of von Neumann algebras of the form \$L({\textbackslash}textbackslashmathcal\{R\})\$ for countable probability measure preserving equivalence relations \${\textbackslash}textbackslashmathcal\{R\}\$. We show that \$L({\textbackslash}textbackslashmathcal\{R\})\$ is prime whenever \${\textbackslash}textbackslashmathcal\{R\}\$ is nonamenable, ergodic, and admits an unbounded 1-cocycle into a mixing orthogonal representation weakly contained in the regular representation. This is accomplished by constructing the \${\textbackslash}textbackslashtextit\{Gaussian extension \}{\textbackslash}textbackslashtilde\{{\textbackslash}textbackslashmathcal\{R\}\}\$ of \${\textbackslash}textbackslashmathcal\{R\}\$ and subsequently an s-malleable deformation of the inclusion \$L({\textbackslash}textbackslashmathcal\{R\}) {\textbackslash}textbackslashsubset L({\textbackslash}textbackslashtilde\{{\textbackslash}textbackslashmathcal\{R\}\})\$. We go on to note a general obstruction to unique prime factorization, and avoiding it, we prove a unique prime factorization result for products of the form \$L({\textbackslash}textbackslashmathcal\{R\}\_1) {\textbackslash}textbackslashotimes L({\textbackslash}textbackslashmathcal\{R\}\_2) {\textbackslash}textbackslashotimes {\textbackslash}textbackslashcdots {\textbackslash}textbackslashotimes L({\textbackslash}textbackslashmathcal\{R\}\_k)\$. As a corollary, we get a unique factorization result in the equivalence relation setting for products of the form \${\textbackslash}textbackslashmathcal\{R\}\_1 {\textbackslash}textbackslashtimes {\textbackslash}textbackslashmathcal\{R\}\_2 {\textbackslash}textbackslashtimes {\textbackslash}textbackslashcdots {\textbackslash}textbackslashtimes {\textbackslash}textbackslashmathcal\{R\}\_k\$. We finish with an application to the measure equivalence of groups.},
	journaltitle = {{arXiv}:1506.00588 [math]},
	author = {Hoff, Daniel J.},
	urldate = {2016-08-12},
	date = {2015-06},
	keywords = {Mathematics - Operator Algebras, Mathematics - Dynamical Systems},
}

