	url = {http://epubs.siam.org/doi/abs/10.1137/S0895479802410293},
	doi = {10.1137/S0895479802410293},
	abstract = {Given a matrix M of size n, the digraph D on n vertices is said to be the digraph {ofM} , when \$M\_\{ij\}{\textbackslash}textbackslashneq 0\$ if and only if (v,sub{\textbackslash}textgreateri,v,sub{\textbackslash}textgreaterj) is an arc of D. We give a necessary condition, called strong quadrangularity, for a digraph to be the digraph of a unitary matrix. With the use of such a condition, we show that a line digraph \${\textbackslash}textbackslashoverrightarrow\{L\}D\$ is the pattern of a unitary matrix if and only if D is Eulerian. It follows that, if D is strongly connected and \${\textbackslash}textbackslashoverrightarrow\{L\}D\$ is the digraph of a unitary matrix, then \${\textbackslash}textbackslashoverrightarrow\{L\}D\$ is Hamiltonian. We observe that strong quadrangularity is sufficient to show that disconnected strongly regular graphs are the digraphs of unitary matrices and that n-paths, n-paths with loops at each vertex, n-cycles, directed trees, and trees are not.},
	pages = {295--300},
	number = {1},
	journaltitle = {{SIAM} Journal on Matrix Analysis and Applications},
	author = {Severini, S.},
	date = {2003-01},
}

