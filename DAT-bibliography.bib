}

@article{rainone_crossed_2016,
	title = {Crossed products of nuclear C*-algebras by free groups and their traces},
	url = {http://arxiv.org/abs/1601.06090},
	abstract = {We study the matricial field ({MF}) property for certain reduced crossed product C*-algebras and their traces. Using classification techniques and induced K-theoretic dynamics, we show that reduced crossed products of {ASH}-algebras of real rank zero by free groups are {MF} if and only if they are stably finite. We also examine traces on these crossed products and show they always admit certain finite dimensional approximation properties. Combining these results with recent progress in Elliott's Classification Program, it follows that if \$A\$ is a separable, simple, unital, nuclear, monotracial C*-algebra satisfying the {UCT}, then \$A {\textbackslash}textbackslashrtimes\_\{{\textbackslash}textbackslashlambda\} {\textbackslash}textbackslashmathbb\{F\}\_r\$ is {MF} for any action \${\textbackslash}textbackslashalpha\$. Appealing to a result of Ozawa, R\{{\textbackslash}textbackslasho\}rdam, and Sato, we show that discrete groups of the form \$G {\textbackslash}textbackslashrtimes {\textbackslash}textbackslashmathbb\{F\}\_r\$ with \$G\$ amenable admit {MF} reduced group C*-algebras In the process, some new permanence properties of {MF} algebras are obtained which are of independent interest. In particular, minimal tensor products of {MF} algebras are again {MF} provided one of the factors is exact.},
	journaltitle = {{arXiv}:1601.06090 [math]},
	author = {Rainone, Timothy and Schafhauser, Christopher},
	urldate = {2017-03-24},
	date = {2016-01},
	keywords = {Mathematics - Operator Algebras},
}

