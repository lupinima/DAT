@article{de_jeu_crossed_2013,
	title = {Crossed products of Banach algebras. {III}},
	url = {http://arxiv.org/abs/1306.6290},
	abstract = {In earlier work a crossed product of a Banach algebra was constructed from a Banach algebra dynamical system \$(A,G,{\textbackslash}textbackslashalpha)\$ and a class \${\textbackslash}textbackslashmathcal\{R\}\$ of continuous covariant representations, and its representations were determined. In this paper we adapt the theory to the ordered context. We construct a pre-ordered crossed product of a Banach algebra from a pre-ordered Banach algebra dynamical system \$(A,G,{\textbackslash}textbackslashalpha)\$ and a given uniformly bounded class \${\textbackslash}textbackslashmathcal\{R\}\$ of continuous covariant representations of \$(A,G,{\textbackslash}textbackslashalpha)\$. If \$A\$ has a positive bounded approximate left identity and \${\textbackslash}textbackslashmathcal\{R\}\$ consists of non-degenerate continuous covariant representations, we establish a bijection between the positive non-degenerate bounded representations of the pre-ordered crossed product on pre-ordered Banach spaces with closed cones and the positive non-degenerate \${\textbackslash}textbackslashmathcal\{R\}\$-continuous covariant representations of \$(A,G,{\textbackslash}textbackslashalpha)\$ on such spaces. Under mild conditions, we show that this pre-ordered crossed product is the essentially unique pre-ordered Banach algebra for which such a bijection exists. Finally, we study pre-ordered generalized Beurling algebras. We show that they are bipositively topologically isomorphic to pre-ordered crossed products of Banach algebras associated with pre-ordered Banach algebra dynamical systems, and hence the general theory allows us to describe their positive representations on pre-ordered Banach spaces with closed cones.},
	journaltitle = {{arXiv}:1306.6290 [math]},
	author = {de Jeu, Marcel and Messerschmidt, Miek},
	urldate = {2014-02-07},
	date = {2013-06},
	keywords = {Mathematics - Functional Analysis, Mathematics - Representation Theory, 46L55, 22D15, 43A20, 46B40, 47B65, Primary 47L65, Secondary 22D12},
}

@article{dirksen_crossed_2011,
	title = {Crossed products of Banach algebras. I},
	url = {http://arxiv.org/abs/1104.5151},
	abstract = {We construct a crossed product Banach algebra from a Banach algebra dynamical system \$(A,G,{\textbackslash}textbackslashalpha)\$ and a given uniformly bounded class \$R\$ of continuous covariant Banach space representations of that system. If \$A\$ has a bounded left approximate identity, and \$R\$ consists of non-degenerate continuous covariant representations only, then the non-degenerate bounded representations of the crossed product are in bijection with the non-degenerate \$R\$-continuous covariant representations of the system. This bijection, which is the main result of the paper, is also established for involutive Banach algebra dynamical systems and then yields the well-known representation theoretical correspondence for the crossed product \$Cˆ*\$-algebra as commonly associated with a \$Cˆ*\$-algebra dynamical system as a special case. Taking the algebra \$A\$ to be the base field, the crossed product construction provides, for a given non-empty class of Banach spaces, a Banach algebra with a relatively simple structure and with the property that its non-degenerate contractive representations in the spaces from that class are in bijection with the isometric strongly continuous representations of \$G\$ in those spaces. This generalizes the notion of a group \$Cˆ*\$-algebra, and may likewise be used to translate issues concerning group representations in a class of Banach spaces to the context of a Banach algebra, simpler than \$Lˆ1(G)\$, where more functional analytic structure is present.},
	journaltitle = {{arXiv}:1104.5151 [math]},
	author = {Dirksen, Sjoerd and de Jeu, Marcel and Wortel, Marten},
	urldate = {2014-02-07},
	date = {2011-04},
	keywords = {Mathematics - Functional Analysis, Mathematics - Representation Theory, 46L55, 22D15, Primary 47L65, 22D12, Secondary 46H25},
}

