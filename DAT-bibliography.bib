@article{baum_noncommutative_2015,
	title = {Noncommutative Borsuk-Ulam-type conjectures},
	url = {http://arxiv.org/abs/1502.05756},
	abstract = {Within the framework of free actions of compact quantum groups on unital C*-algebras, we propose two conjectures. The first one states that, if \$H\$ is the C*-algebra of a compact quantum group coacting freely on a unital C*-algebra \$A\$, then there is no equivariant \$*\$-homomorphism from \$A\$ to the join C*-algebra \$A*H\$. For \$A\$ being the C*-algebra of continuous functions on a sphere with the antipodal coaction of the C*-algebra of funtions on \${\textbackslash}textbackslashmathbb\{Z\}/2{\textbackslash}textbackslashmathbb\{Z\}\$, we recover the celebrated Borsuk-Ulam theorem. The second conjecture states that there is no equivariant \$*\$-homomorphism from \$H\$ to the join C*-algebra \$A*H\$. We show how to prove the conjecture in the special case \$A=C({SU}\_q(2))=H\$, which is tantamount to showing the non-trivializability of Pflaum's quantum instanton fibration built from \${SU}\_q(2)\$.},
	journaltitle = {{arXiv}:1502.05756 [math-ph]},
	author = {Baum, Paul F. and Dabrowski, Ludwik and Hajac, Piotr M.},
