@article{bodmann_decoherence-insensitive_2007,
	title = {Decoherence-insensitive quantum communication by optimal C*-encoding},
	volume = {53},
	issn = {0018-9448},
	doi = {10.1109/TIT.2007.909105},
	abstract = {The central issue in this paper is to transmit a quantum state in such a way that after some decoherence occurs, most of the information can be restored by a suitable decoding operation. For this purpose, we incorporate redundancy by mapping a given initial quantum state to a messenger state on a larger dimensional Hilbert space via a C* -algebra embedding. Our noise model for the transmission is a phase damping channel which admits a noiseless subsystem or decoherence-free subspace. More precisely, the transmission channel is obtained from convex combinations of a set of lowest rank yes/no measurements that leave a component of the messenger state unchanged. The objective of our encoding is to distribute quantum information optimally across the noise-susceptible component of the transmission when the noiseless component is not large enough to contain all the quantum information to be transmitted. We derive simple geometric conditions for optimal encoding and construct examples of such encodings.},
	pages = {4738--4749},
	number = {12},
	journaltitle = {{IEEE} Transactions on Information Theory},
	author = {Bodmann, Bernhard G. and Kribs, David W. and Paulsen, Vern I.},
	date = {2007-12},
	keywords = {Mathematics, quantum information, quantum communication, $^{\textrm{ast \$}}$\$-algebra embedding, 03.67.Pp, 81R15 (secondary), 94A24, algebraic codes, channel coding, codes, Cryptography, Damping, decoding, decoding operation, decoherence-free subspace, decoherence-insensitive quantum communication, Error correction, geometric condition, Hilbert space, Hilbert spaces, Image restoration, larger-dimensional Hilbert space, messenger state, {MSC} (2000): 81P68 (primary), noise-susceptible component, noiseless subsystem, optimal C* -encoding, {PACS} numbers: 03.67.Hk, phase damping channel, Phase noise, Quantum computing, quantum state, Redundancy, transmission channel},
