	author = {Uspenskij, Vladimir V.},
	urldate = {2014-02-01},
	date = {2008-08},
	keywords = {Urysohn metric space, idempotent, uniformity, Isometry, Roelcke compactification, Semigroup, Topological group, Unitary group},
}

