	keywords = {Mathematics - Logic, Mathematics - Algebraic Topology, Mathematics - K-Theory and Homology, Mathematics - Geometric Topology, 54N05, 55P15, 03E15 (Primary), 18G99, 55N99, 54D45, 55P99 (Secondary)},
	file = {arXiv Fulltext PDF:C\:\\Users\\Martino Lupini\\Zotero\\storage\\2NP3CDM9\\Bergfalk et al. - 2022 - The definable content of homological invariants II.pdf:application/pdf},
