@thesis{faonte_nerve_nodate,
	location = {United States -- Connecticut},
	title = {Nerve construction, A-infinity functors and homotopy theory of differential graded categories},
	rights = {Database copyright {ProQuest} {LLC}; {ProQuest} does not claim copyright in the individual underlying works.},
	url = {http://www.proquest.com/docview/1814764426/abstract/BF23944F38B44979PQ/1},
	abstract = {In this dissertation we prove that the derived enrichment of the homotopy category of differential graded categories (dg-categories) with respect to a certain model category structure defined by G. Tabuada, is computed by the dg-category of A-infinity functors. This result was originally formulated by M. Kontsevich but rio formal proof was available in the literature. We further put this construction into the framework of higher categories. We introduce a functor, called the simplicial nerve of an A-infinity category, defined on the category of (small) A-infinity categories and taking values in simplicial sets. This construction extends the nerve construction for dg-categories of J. Lurie. We prove that the nerve of any A-infinity category is an (∞,1)-category and that the nerve of a pretriangulated dg-category in the sense of A. Bondal and M. Kapranov is a stable (∞, 1)-category in the sense of J. Lurie. We then provide a series of applications of these results. We introduce an enhancement of the categories {dgCat} and A∞ Cat, of dg and A-infinity categories, to (∞, 2)-categories. We show that the (∞, 1)-category associated to the (∞, 2)-category of dg-categories is a model for the simplicial localization at the class of weak-equivalences of the Tabuada's model structure. Finally, we prove that the homotopy groups of the self-mapping space of the identity functor in the (∞, 2)-category of A-infinity categories compute the Hochschild cohomology for A-infinity categories.},
	pagetotal = {156},
	institution = {Yale University},
	type = {phdthesis},
	author = {Faonte, Giovanni},
	urldate = {2022-11-18},
	note = {{ISBN}: 9781369091137},
	keywords = {Pure sciences, A-Infinity, {DG}-Categories, Enrichment, Higher Categories, Nerve},
	file = {Faonte - Nerve construction, A-infinity functors and homoto.pdf:C\:\\Users\\Martino Lupini\\Zotero\\storage\\UQ382HW3\\Faonte - Nerve construction, A-infinity functors and homoto.pdf:application/pdf;Full Text PDF:C\:\\Users\\Martino Lupini\\Zotero\\storage\\YGMXW69Z\\Faonte - Nerve construction, A-infinity functors and homoto.pdf:application/pdf},
