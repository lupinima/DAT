	doi = {10.1007/s00220-008-0661-8},
	abstract = {We show that states on tensor products of matrix algebras whose ranks are relatively small are almost surely entangled, but that states of maximum rank are not. More precisely, let M=Mm(C)M = M\_m({\textbackslash}textbackslashmathbb\{C\}) and N=Mn(C)\{N=M\_n({\textbackslash}textbackslashmathbb C)\} be full matrix algebras with m ≥ n, fix an arbitrary state ω of N, and let E(ω) be the set of all states of M⊗N\{M{\textbackslash}textbackslashotimes N\} that extend ω. The space E(ω) contains states of rank r for every r = 1, 2, . . . , m · rank ω, and it has a filtration into compact subspaces E1(ω)⊆E2(ω)⊆⋯⊆Em⋅rankω=E(ω),Eˆ1({\textbackslash}textbackslashomega){\textbackslash}textbackslashsubseteq Eˆ2({\textbackslash}textbackslashomega){\textbackslash}textbackslashsubseteq {\textbackslash}textbackslashcdots{\textbackslash}textbackslashsubseteq Eˆ\{m{\textbackslash}textbackslashcdot\{{\textbackslash}textbackslashrm rank\}{\textbackslash}textbackslash,{\textbackslash}textbackslashomega\}=E({\textbackslash}textbackslashomega), where E r (ω) is the set of all states of E(ω) having rank ≤ r. We show first that for every r, there is a real-analytic manifold V r , homogeneous under a transitive action of a compact group G r , which parameterizes E r (ω). The unique G r -invariant probability measure on V r promotes to a probability measure P r,ω on E r (ω), and P r,ω assigns probability 1 to states of rank r. The resulting probability space (E r (ω),P r,ω ) represents “choosing a rank r extension of ω at random”. Main result: For every r = 1, 2, . . . , [rank ω/2], states of (E r (ω),P r,ω ) are almost surely entangled.},
	pages = {283--312},
	number = {1},
	journaltitle = {Communications in Mathematical Physics},
	author = {Arveson, William},
	urldate = {2015-03-21},
	date = {2008-10},
	langid = {english},
	keywords = {Quantum Physics, Mathematical and Computational Physics, Statistical Physics, Complexity, Information and Physics, Quantum Computing, Relativity and Cosmology},
}

