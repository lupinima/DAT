@article{elliott_automatic_2021,
	title = {Automatic continuity, unique Polish topologies, and Zariski topologies on monoids and clones},
	url = {http://arxiv.org/abs/1912.07029},
	abstract = {In this paper we explore the extent to which the algebraic structure of a monoid \$M\$ determines the topologies on \$M\$ that are compatible with its multiplication. Specifically we study the notions of automatic continuity; minimal Hausdorff or Polish semigroup topologies; and we formulate a notion of the Zariski topology for monoids. If \$M\$ is a topological monoid such that every homomorphism from \$M\$ to a second countable topological monoid \$N\$ is continuous, then we say that \$M\$ has {\textbackslash}emph\{automatic continuity\}. We show that many well-known monoids have automatic continuity with respect to a natural semigroup topology, namely: the full transformation monoid \${\textbackslash}mathbb\{N\}{\textasciicircum}{\textbackslash}mathbb\{N\}\$; the full binary relation monoid \$B\_\{{\textbackslash}mathbb\{N\}\}\$; the partial transformation monoid \$P\_\{{\textbackslash}mathbb\{N\}\}\$; the symmetric inverse monoid \$I\_\{{\textbackslash}mathbb\{N\}\}\$; the monoid Inj\$({\textbackslash}mathbb\{N\})\$ consisting of the injective functions on \${\textbackslash}mathbb\{N\}\$; and the monoid \$C(2{\textasciicircum}\{{\textbackslash}mathbb\{N\}\})\$ of continuous functions on the Cantor set. We show that the pointwise topology on \${\textbackslash}mathbb\{N\}{\textasciicircum}{\textbackslash}mathbb\{N\}\$, and its analogue on \$P\_\{{\textbackslash}mathbb\{N\}\}\$, are the unique Polish semigroup topologies on these monoids. The compact-open topology is the unique Polish semigroup topology on \$C(2{\textasciicircum}{\textbackslash}mathbb\{N\})\$ and \$C([0, 1]{\textasciicircum}{\textbackslash}mathbb\{N\})\$. There are at least 3 Polish semigroup topologies on \$I\_\{{\textbackslash}mathbb\{N\}\}\$, but a unique Polish inverse semigroup topology. There are no Polish semigroup topologies \$B\_\{{\textbackslash}mathbb\{N\}\}\$ nor on the partitions monoids. At the other extreme, Inj\$({\textbackslash}mathbb\{N\})\$ and the monoid Surj\$({\textbackslash}mathbb\{N\})\$ of all surjective functions on \${\textbackslash}mathbb\{N\}\$ each have infinitely many distinct Polish semigroup topologies. We prove that the Zariski topologies on \${\textbackslash}mathbb\{N\}{\textasciicircum}{\textbackslash}mathbb\{N\}\$, \$P\_\{{\textbackslash}mathbb\{N\}\}\$, and Inj\$({\textbackslash}mathbb\{N\})\$ coincide with the pointwise topology; and we characterise the Zariski topology on \$B\_\{{\textbackslash}mathbb\{N\}\}\$. In Section 7: clones.},
	journaltitle = {{arXiv}:1912.07029 [math]},
	author = {Elliott, L. and Jonušas, J. and Mesyan, Z. and Mitchell, J. D. and Morayne, M. and Péresse, Y.},
	urldate = {2022-02-07},
	date = {2021-01-18},
	eprinttype = {arxiv},
	eprint = {1912.07029},
	keywords = {Mathematics - Group Theory, 20M20, 20M30, 22A15, 54H11, Mathematics - Rings and Algebras},
	file = {arXiv Fulltext PDF:C\:\\Users\\Martino Lupini\\Zotero\\storage\\4Z898TRP\\Elliott et al. - 2021 - Automatic continuity, unique Polish topologies, an.pdf:application/pdf},
