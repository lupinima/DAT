}

@article{conti_weyl_2012,
	title = {The Weyl group of the Cuntz algebra},
	volume = {231},
	issn = {0001-8708},
	url = {http://www.sciencedirect.com/science/article/pii/S000187081200326X},
	doi = {10.1016/j.aim.2012.09.003},
	abstract = {The Weyl group of the Cuntz algebra {OnOn} is investigated. This is (isomorphic to) the group of polynomial automorphisms λuλu of {OnOn}, namely those induced by unitaries uu that can be written as finite sums of words in the canonical generating isometries {SiSi} and their adjoints. A necessary and sufficient algorithmic combinatorial condition is found for deciding when a polynomial endomorphism λuλu restricts to an automorphism of the canonical diagonal {MASA}. Some steps towards a general criterion for invertibility of λuλu on the whole of {OnOn} are also taken. A condition for verifying invertibility of a certain subclass of polynomial endomorphisms is given. First examples of polynomial automorphisms of {OnOn} not inner related to those induced by unitaries from the core {UHF} subalgebra are exhibited, for every n≥2n≥2. In particular, the image of the Weyl group in the outer automorphism group of {OnOn} is strictly larger than the image of the restricted Weyl group analyzed in previous papers. Results about the action of the Weyl group on the spectrum of the diagonal are also included.},
	pages = {3147--3161},
	number = {6},
	journaltitle = {Advances in Mathematics},
	author = {Conti, Roberto and Hong, Jeong Hee and Szymański, Wojciech},
