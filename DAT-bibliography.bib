@article{janzing_non-identity-check_2005,
	title = {"Non-identity-check" is {QMA}-complete},
	volume = {3},
	issn = {0219-7499},
	url = {http://www.worldscientific.com/doi/abs/10.1142/S0219749905001067},
	doi = {10.1142/S0219749905001067},
	abstract = {We describe a computational problem that is complete for the complexity class {QMA}, a quantum generalization of {NP}. It arises as a natural question in quantum computing and quantum physics. "Non-identity-check" is the following decision problem: Given a classical description of a quantum circuit (a sequence of elementary gates), determine whether it is almost equivalent to the identity. Explicitly, the task is to decide whether the corresponding unitary is close to a complex multiple of the identity matrix with respect to the operator norm. We show that this problem is {QMA}-complete. A generalization of this problem is "non-equivalence check": given two descriptions of quantum circuits and a description of a common invariant subspace, decide whether the restrictions of the circuits to this subspace almost coincide. We show that non-equivalence check is also in {QMA} and hence {QMA}-complete.},
