@article{messerschmidt_normality_2015,
	title = {Normality of spaces of operators and quasi-lattices},
	volume = {19},
	issn = {1385-1292, 1572-9281},
	url = {http://arxiv.org/abs/1307.1415},
	doi = {10.1007/s11117-015-0323-y},
	abstract = {We give an overview of normality and conormality properties of pre-ordered Banach spaces. For pre-ordered Banach spaces \$X\$ and \$Y\$ with closed cones we investigate normality of \$B(X,Y)\$ in terms of normality and conormality of the underlying spaces \$X\$ and \$Y\$. Furthermore, we define a class of ordered Banach spaces called quasi-lattices which strictly contains the Banach lattices, and we prove that every strictly convex reflexive ordered Banach space with a closed proper generating cone is a quasi-lattice. These spaces provide a large class of examples \$X\$ and \$Y\$ that are not Banach lattices, but for which \$B(X,Y)\$ is normal. In particular, we show that a Hilbert space \${\textbackslash}textbackslashmathcal\{H\}\$ endowed with a Lorentz cone is a quasi-lattice (that is not a Banach lattice if \${\textbackslash}textbackslashdim{\textbackslash}textbackslashmathcal\{H\}{\textbackslash}textbackslashgeq3\$), and satisfies an identity analogous to the elementary Banach lattice identity \${\textbackslash}textbackslash{\textbackslash}textbar{\textbackslash}textbarx{\textbackslash}textbar{\textbackslash}textbackslash{\textbackslash}textbar={\textbackslash}textbackslash{\textbackslash}textbarx{\textbackslash}textbackslash{\textbackslash}textbar\$ which holds for all elements \$x\$ of a Banach lattice. This is used to show that spaces of operators between such ordered Hilbert spaces are always absolutely monotone and that the operator norm is positively attained, as is also always the case for spaces of operators between Banach lattices.},
	pages = {695--724},
	number = {4},
	journaltitle = {Positivity},
	author = {Messerschmidt, Miek},
	urldate = {2016-08-05},
	date = {2015-12},
	keywords = {Mathematics - Functional Analysis, 46A40, 46B42, 47H07, Primary: 46B40 Secondary: 47B60},
}

