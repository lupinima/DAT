@article{mesyan_topological_2018,
	title = {Topological Transformation Monoids},
	url = {http://arxiv.org/abs/1809.04590},
	abstract = {We investigate semigroup topologies on the full transformation monoid T(X) of an infinite set X. We show that the standard pointwise topology is the weakest Hausdorff semigroup topology on T(X), show that the pointwise topology is the unique Hausdorff semigroup topology on T(X) that induces the pointwise topology on the group of all permutations of X, and construct {\textbackslash}{textbarX}{\textbackslash}textbar distinct Hausdorff semigroup topologies on T(X). In the case where X is countable, we prove that the pointwise topology is the only Polish semigroup topology on T(X). We also show that every separable semigroup topology on T(X) is perfect, describe the compact sets in an arbitrary Hausdorff semigroup topology on T(X), and show that there are no locally compact perfect Hausdorff semigroup topologies on T(X) when {\textbackslash}{textbarX}{\textbackslash}textbar has uncountable cofinality.},
	journaltitle = {{arXiv}:1809.04590 [math]},
	author = {Mesyan, Z. and Mitchell, J. D. and Péresse, Y. H.},
	urldate = {2020-03-17},
	date = {2018-09},
}

@article{elliott_automatic_2019,
	title = {Automatic continuity and uniqueness of Polish semigroup topologies},
	url = {http://arxiv.org/abs/1912.07029},
	abstract = {If \$S\$ is a topological monoid such that every homomorphism from \$S\$ to a second countable topological monoid \$T\$ is continuous, then we say that \$S\$ has \$automatic\$ \$continuity\$. In this paper, we show that many well-known, and extensively studied, monoids have automatic continuity with respect to some natural semigroup topology. Namely, the following have automatic continuity: the monoid \$B\_\{{\textbackslash}textbackslashmathbb\{N\}\}\$ of all binary relations on the natural numbers \${\textbackslash}textbackslashmathbb\{N\}\$; the monoid \$P\_\{{\textbackslash}textbackslashmathbb\{N\}\}\$ of partial transformations; the full transformation monoid \${\textbackslash}textbackslashmathbb\{N\} ˆ {\textbackslash}textbackslashmathbb\{N\}\$; the monoid \${\textbackslash}textbackslashoperatorname\{Inj\}({\textbackslash}textbackslashmathbb\{N\})\$ of injective transformations on \${\textbackslash}textbackslashmathbb\{N\}\$; the symmetric inverse monoid \$I\_\{{\textbackslash}textbackslashmathbb\{N\}\}\$; and the monoid \$C(2 ˆ {\textbackslash}textbackslashmathbb\{N\})\$ of continuous functions on the Cantor set \$2 ˆ {\textbackslash}textbackslashmathbb\{N\}\$. Additionally, we show that: \$B\_\{{\textbackslash}textbackslashmathbb\{N\}\}\$ has no Polish semigroup topologies; \$P\_\{{\textbackslash}textbackslashmathbb\{N\}\}\$, \$C(2 ˆ {\textbackslash}textbackslashmathbb\{N\})\$, and the monoid \$C([0, 1] ˆ {\textbackslash}textbackslashmathbb\{N\})\$ of continuous functions on the Hilbert cube \$[0, 1] ˆ {\textbackslash}textbackslashmathbb\{N\}\$ each have a unique Polish semigroup topology; the monoid \${\textbackslash}textbackslashoperatorname\{End\}({\textbackslash}textbackslashmathbb\{Q\}, {\textbackslash}textbackslashleq)\$ of all order-endomorphisms of the rational numbers, and the monoid of endomorphisms of the countable random graph (the Rado graph) have at least one Polish semigroup topology; the monoid of self-embeddings of any Fra{\textbackslash}textbackslash"iss{\textbackslash}textbackslash'e limit which admits proper embeddings has at least two Polish semigroup topologies; \$I\_\{{\textbackslash}textbackslashmathbb\{N\}\}\$ has at least 3 Polish semigroup topologies, but a unique Polish inverse semigroup topology; and \${\textbackslash}textbackslashoperatorname\{Inj\}({\textbackslash}textbackslashmathbb\{N\})\$ and the monoid \${\textbackslash}textbackslashoperatorname\{Surj\}({\textbackslash}textbackslashmathbb\{N\})\$ of all surjective transformations of \${\textbackslash}textbackslashmathbb\{N\}\$ have infinitely many distinct Polish semigroup topologies. In proving the main results mentioned above we prove myriad ancillary results.},
	journaltitle = {{arXiv}:1912.07029 [math]},
	author = {Elliott, L. and Jonušas, J. and Mitchell, J. D. and Morayne, M. and Péresse, Y.},
	urldate = {2020-03-17},
	date = {2019-12},
}

