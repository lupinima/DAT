@article{benaych-georges_singular_2011,
	title = {The singular values and vectors of low rank perturbations of large rectangular random matrices},
	url = {http://arxiv.org/abs/1103.2221},
	abstract = {In this paper, we consider the singular values and singular vectors of finite, low rank perturbations of large rectangular random matrices. Specifically, we prove almost sure convergence of the extreme singular values and appropriate projections of the corresponding singular vectors of the perturbed matrix. As in the prequel, where we considered the eigenvalue aspect of the problem, the non-random limiting value is shown to depend explicitly on the limiting singular value distribution of the unperturbed matrix via an integral transforms that linearizes rectangular additive convolution in free probability theory. The large matrix limit of the extreme singular values of the perturbed matrix differs from that of the original matrix if and only if the singular values of the perturbing matrix are above a certain critical threshold which depends on this same aforementioned integral transform. We examine the consequence of this singular value phase transition on the associated left and right singular eigenvectors and discuss the finite \$n\$ fluctuations above these non-random limits.},
	journaltitle = {{arXiv}:1103.2221 [math]},
	author = {Benaych-Georges, Florent and Nadakuditi, Raj Rao},
	date = {2011-03},
}

@article{elek_connes_2010,
	title = {Connes Embeddings and von Neumann Regular Closures of Group Algebras},
	url = {http://arxiv.org/abs/1006.5378},
	abstract = {The analytic von Neumann regular closure \$R({\textbackslash}{textbackslashGamma})\$ of a complex group algebra \${\textbackslash}{textbackslashC}{\textbackslash}{textbackslashGamma}\$ was introduced by Linnell and Schick. This ring is the smallest \$*\$-regular subring in the algebra of affiliated operators \$U({\textbackslash}{textbackslashGamma})\$ containing \${\textbackslash}{textbackslashC}{\textbackslash}{textbackslashGamma}\$. We prove that all the algebraic von Neumann regular closures corresponding to sofic representations of an amenable group are isomorphic to \$R({\textbackslash}{textbackslashGamma})\$. This result can be viewed as a structural generalization of L{\textbackslash}textbackslash"uck's Approximation Theorem. {\textbackslash}textbackslashnoindent The main tool of the proof which might be of independent interest is that an amenable group algebra \$K{\textbackslash}{textbackslashGamma}\$ over any field \$K\$ can be embedded to the rank completion of an ultramatricial algebra.},
	journaltitle = {{arXiv}:1006.5378 [math]},
	author = {Elek, Gabor},
	date = {2010-06},
}

@article{elek_lamplighter_2014,
