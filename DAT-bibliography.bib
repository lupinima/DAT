@article{gabe_absorbing_2015,
	title = {Absorbing representations with respect to closed operator convex cones},
	url = {http://arxiv.org/abs/1503.07799},
	abstract = {We initiate the study of absorbing representations of \$Cˆ{\textbackslash}textbackslashast\$-algebras with respect to closed operator convex cones. We completely determine when such absorbing representations exist, which leads to the question of characterising when a representation is absorbing, as in the classical Weyl-von Neumann type theorem of Voiculescu. In the classical case, this was proven by Elliott and Kucerovsky who proved that a representation is nuclearly absorbing if and only if it induces a purely large extension. By considering a related problem for extensions of \$Cˆ{\textbackslash}textbackslashast\$-algebras, which we call the purely large problem, we ask when a purely largeness condition similar to the one defined by Elliott and Kucerovsky, implies absorption with respect to some given closed operator convex cone. We solve this question for a special type of closed operator convex cone induced by actions of finite topological spaces on \$Cˆ{\textbackslash}textbackslashast\$-algebras. As an application of this result, we give \$K\$-theoretic classification for certain \$Cˆ{\textbackslash}textbackslashast\$-algebras containing a purely infinite, two-sided, closed ideal for which the quotient is an {AF} algebra. This generalises a similar result by the second author, S. Eilers and G. Restorff in which all extensions had to be full.},
	journaltitle = {{arXiv}:1503.07799 [math]},
	author = {Gabe, James and Ruiz, Efren},
