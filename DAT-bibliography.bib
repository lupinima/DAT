@misc{noauthor_whitehead_2019,
	title = {Whitehead manifold},
	rights = {Creative Commons Attribution-{ShareAlike} License},
	url = {https://en.wikipedia.org/w/index.php?title=Whitehead_manifold&oldid=887629604},
	abstract = {In mathematics, the Whitehead manifold is an open 3-manifold that is contractible, but not homeomorphic to R 3 \{{\textbackslash}textbackslashdisplaystyle {\textbackslash}textbackslashmathbb \{R\} ˆ\{3\}\} . J. H. C. Whitehead (1935) discovered this puzzling object while he was trying to prove the Poincaré conjecture, correcting an error in an earlier paper Whitehead (1934, theorem 3) where he incorrectly claimed that no such manifold exists. A contractible manifold is one that can continuously be shrunk to a point inside the manifold itself. For example, an open ball is a contractible manifold. All manifolds homeomorphic to the ball are contractible, too. One can ask whether all contractible manifolds are homeomorphic to a ball. For dimensions 1 and 2, the answer is classical and it is "yes". In dimension 2, it follows, for example, from the Riemann mapping theorem. Dimension 3 presents the first counterexample: the Whitehead manifold.},
	urldate = {2019-11-03},
