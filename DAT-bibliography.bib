	url = {http://arxiv.org/abs/math/0606541},
	abstract = {Starting with a unit-preserving normal completely positive map L: M –{\textbackslash}textgreater M acting on a von Neumann algebra - or more generally a dual operator system - we show that there is a unique reversible system {\textbackslash}textbackslashalpha: N –{\textbackslash}textgreater N (i.e., a complete order automorphism {\textbackslash}textbackslashalpha of a dual operator system N) that captures all of the asymptotic behavior of L, called the \{{\textbackslash}textbackslashem asymptotic lift\} of L. This provides a noncommutative generalization of the Frobenius theorems that describe the asymptotic behavior of the sequence of powers of a stochastic n x n matrix. In cases where M is a von Neumann algebra, the asymptotic lift is shown to be a W*-dynamical system (N,{\textbackslash}textbackslashmathbb Z), whick we identify as the tail flow of the minimal dilation of L. We are also able to identify the Poisson boundary of L as the fixed point algebra of (N,{\textbackslash}textbackslashmathbb Z). In general, we show the action of the asymptotic lift is trivial iff L is \{{\textbackslash}textbackslashem slowly oscillating\} in the sense that \$\$ {\textbackslash}textbackslashlim\_\{n{\textbackslash}textbackslashto{\textbackslash}textbackslashinfty\}{\textbackslash}textbackslash{\textbackslash}textbar{\textbackslash}textbackslashrho{\textbackslash}textbackslashcirc Lˆ\{n+1\}-{\textbackslash}textbackslashrho{\textbackslash}textbackslashcirc Lˆn{\textbackslash}textbackslash{\textbackslash}textbar=0,{\textbackslash}textbackslashqquad {\textbackslash}textbackslashrho{\textbackslash}textbackslashin M\_* . \$\$ Hence {\textbackslash}textbackslashalpha is often a nontrivial automorphism of N.},
	journaltitle = {{arXiv}:math/0606541},
	author = {Arveson, William},
	urldate = {2015-08-08},
	date = {2006-06},
}

