@article{bowen_cheeger_2013,
	title = {Cheeger constants and \$Lˆ2\$-Betti numbers},
	url = {http://arxiv.org/abs/1303.5963},
	abstract = {We prove the existence of positive lower bounds on the Cheeger constants of manifolds of the form \$X/{\textbackslash}{textbackslashGamma}\$ where \$X\$ is a contractible Riemannian manifold and \${\textbackslash}{textbackslashGamma}{\textbackslash}textless{\textbackslash}{textbackslashIsom}(X)\$ is a discrete subgroup, typically with infinite co-volume. The existence depends on the \$Lˆ2\$-Betti numbers of \${\textbackslash}{textbackslashGamma}\$, its subgroups and of a uniform lattice of \${\textbackslash}{textbackslashIsom}(X)\$. As an application, we show the existence of a uniform positive lower bound on the Cheeger constant of any manifold of the form \${\textbackslash}{textbackslashHˆ}4/{\textbackslash}{textbackslashGamma}\$ where \${\textbackslash}{textbackslashHˆ}4\$ is real hyperbolic 4-space and \${\textbackslash}{textbackslashGamma}{\textbackslash}textless{\textbackslash}{textbackslashIsom}({\textbackslash}{textbackslashHˆ}4)\$ is discrete and isomorphic to a subgroup of the fundamental group of a complete finite-volume hyperbolic 3-manifold. Via Patterson-Sullivan theory, this implies the existence of a uniform positive upper bound on the Hausdorff dimension of the conical limit set of such a \${\textbackslash}{textbackslashGamma}\$ when \${\textbackslash}{textbackslashGamma}\$ is geometrically finite. Another application shows the existence of a uniform positive lower bound on the zero-th eigenvalue of the Laplacian of \${\textbackslash}{textbackslashHˆn}/{\textbackslash}{textbackslashGamma}\$ over all discrete free groups \${\textbackslash}{textbackslashGamma}{\textbackslash}textless{\textbackslash}{textbackslashIsom}({\textbackslash}{textbackslashHˆn})\$ whenever \$n{\textbackslash}textbackslashge 4\$ is even (the bound depends on \$n\$). This extends results of Phillips-Sarnak and Doyle who obtained such bounds for \$n{\textbackslash}textbackslashge 3\$ when \${\textbackslash}{textbackslashGamma}\$ is a finitely generated Schottky group.},
