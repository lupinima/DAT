@article{berg_quasilinearization_2008,
	title = {Quasilinearization and curvature of Aleksandrov spaces},
	volume = {133},
	issn = {0046-5755, 1572-9168},
	url = {http://link.springer.com/article/10.1007/s10711-008-9243-3},
	doi = {10.1007/s10711-008-9243-3},
	abstract = {We present a new distance characterization of Aleksandrov spaces of non-positive curvature. By introducing a quasilinearization for abstract metric spaces we draw an analogy between characterization of Aleksandrov spaces and inner product spaces; the quasi-inner product is defined by means of the quadrilateral cosine—a metric substitute for the angular measure between two directions at different points. Our main result states that a geodesically connected metric space {TeX}\{{\textbackslash}textbackslashleft({\textbackslash}textbackslashmathcal\{M\},{\textbackslash}textbackslashrho{\textbackslash}textbackslashright)\} is an Aleksandrov {TeX}\{{\textbackslash}{textbackslashRe}\_\{0\}\} domain (also known as a {CAT}(0) space) if and only if the quadrilateral cosine does not exceed one for every two pairs of distinct points in {TeX}\{{\textbackslash}textbackslashmathcal\{M\}\} . We also observe that a geodesically connected metric space {TeX}\{{\textbackslash}textbackslashleft({\textbackslash}textbackslashmathcal\{M\},{\textbackslash}textbackslashrho{\textbackslash}textbackslashright)\} is an {TeX}\{{\textbackslash}{textbackslashRe}\_\{0\}\} domain if and only if, for every quadruple of points in {TeX}\{{\textbackslash}textbackslashmathcal\{M\}\} , the quadrilateral inequality (known as Euler’s inequality in {TeX}\{{\textbackslash}textbackslashmathbb\{R\}ˆ\{2\}\} ) holds. As a corollary of our main result we give necessary and sufficient conditions for a semimetric space to be an {TeX}\{{\textbackslash}{textbackslashRe}\_\{0\}\} domain. Our results provide a complete solution to the Curvature Problem posed by Gromov in the context of metric spaces of non-positive curvature.},
	pages = {195--218},
	number = {1},
	journaltitle = {Geometriae Dedicata},
	author = {Berg, I. D. and Nikolaev, I. G.},
	urldate = {2015-05-09},
	date = {2008-03},
	langid = {english},
	keywords = {Geometry, 2-Roundness, 51K10, 53C20, 53C45, Aleksandrov space, Quadrilateral cosine, Quadrilateral inequality, {TeX}\{{\textbackslash}{textbackslashRe}\_\{0\}\} domain},
}

