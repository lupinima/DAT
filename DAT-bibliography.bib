@article{suzuki_minimal_2015,
	title = {Minimal ambient nuclear C*-algebras},
	url = {http://arxiv.org/abs/1510.05592},
	abstract = {We study generic properties of Cantor systems for infinite free product groups. As the first consequence, we obtain examples of prime amenable minimal dynamical systems of non-amenable groups. In fact, we show that a property stronger than primeness holds on a \$G\_{\textbackslash}textbackslashdelta\$-dense set of dynamical systems. Using this property, we then show that any infinite free product group with the {AP} (approximation property) admits an amenable dynamical system with the following property. There are no proper intermediate C*-algebras between the reduced group C*-algebra and the reduced crossed product. In particular this gives the first example of a minimal ambient nuclear C*-algebra of a non-nuclear C*-algebra. The proof is based on the Powers argument.},
	journaltitle = {{arXiv}:1510.05592 [math]},
	author = {Suzuki, Yuhei},
	urldate = {2016-02-15},
	date = {2015-10},
}

@article{glasner_uniformly_2014,
	title = {Uniformly recurrent subgroups},
	url = {http://arxiv.org/abs/1402.5028},
	abstract = {We define the notion of uniformly recurrent subgroup, {URS} in short, which is a topological analog of the notion of invariant random subgroup ({IRS}), introduced in a work of M. Abert, Y. Glasner and B. Virag. Our main results are as follows. (i) It was shown by B. Weiss that for an arbitrary countable infinite group G, any free ergodic probability measure preserving G-system admits a minimal model. In contrast we show here, using {URS}'s, that for the lamplighter group there is an ergodic measure preserving action which does not admit a minimal model. (ii) For an arbitrary countable group G, every {URS} can be realized as the stability system of some topologically transitive G-system.},
	journaltitle = {{arXiv}:1402.5028 [math]},
	author = {Glasner, Eli and Weiss, Benjamin},
	urldate = {2016-02-13},
	date = {2014-02},
	keywords = {Mathematics - Group Theory, Mathematics - Dynamical Systems, 37B05, 20E15, 37A15, 20E05, 57S20},
}

@article{bjorklund_product_2015,
