	url = {http://arxiv.org/abs/1402.5499},
	abstract = {Let \${\textbackslash}{textbackslashGamma}\$ be a discrete group. Following Linnell and Schick one can define a continuous ring \$c({\textbackslash}{textbackslashGamma})\$ associated with \${\textbackslash}{textbackslashGamma}\$. They proved that if the Atiyah Conjecture holds for a torsion-free group \${\textbackslash}{textbackslashGamma}\$, then \$c({\textbackslash}{textbackslashGamma})\$ is a skew field. Also, if \${\textbackslash}{textbackslashGamma}\$ has torsion and the Strong Atiyah Conjecture holds for \${\textbackslash}{textbackslashGamma}\$, then \$c({\textbackslash}{textbackslashGamma})\$ is a matrix ring over a skew field. The simplest example when the Strong Atiyah Conjecture fails is the lamplighter group \${\textbackslash}{textbackslashGamma}=Z\_2{\textbackslash}textbackslashwr Z\$. It is known that \$C(Z\_2{\textbackslash}textbackslashwr Z)\$ does not even have a classical ring of quotients. Our main result is that if \$H\$ is amenable, then \$c(Z\_2{\textbackslash}textbackslashwr H)\$ is isomorphic to a continuous ring constructed by John von Neumann in the \$1930's\$.},
	journaltitle = {{arXiv}:1402.5499 [math]},
	author = {Elek, Gabor},
	date = {2014-02},
}

