@article{ioana_strong_2014,
	title = {Strong ergodicity, property (T), and orbit equivalence rigidity for translation actions},
	url = {http://arxiv.org/abs/1406.6628},
	abstract = {We study equivalence relations that arise from translation actions \${\textbackslash}{textbackslashGamma}{\textbackslash}textbackslashcurvearrowright G\$ which are associated to dense embeddings \${\textbackslash}{textbackslashGamma}{\textbackslash}{textlessG}\$ of countable groups into second countable locally compact groups. Assuming that \$G\$ is simply connected and the action \${\textbackslash}{textbackslashGamma}{\textbackslash}textbackslashcurvearrowright G\$ is strongly ergodic, we prove that \${\textbackslash}{textbackslashGamma}{\textbackslash}textbackslashcurvearrowright G\$ is orbit equivalent to another such translation action \${\textbackslash}{textbackslashLambda}{\textbackslash}textbackslashcurvearrowright H\$ if and only if there exists an isomorphism \${\textbackslash}textbackslashdelta:G{\textbackslash}textbackslashrightarrow H\$ such that \${\textbackslash}textbackslashdelta({\textbackslash}{textbackslashGamma})={\textbackslash}{textbackslashLambda}\$. If \$G\$ is moreover a real algebraic group, then we establish analogous rigidity results for the translation actions of \${\textbackslash}{textbackslashGamma}\$ on homogeneous spaces of the form \$G/{\textbackslash}{textbackslashSigma}\$, where \${\textbackslash}{textbackslashSigma}{\textbackslash}{textlessG}\$ is either a discrete or an algebraic subgroup. We also prove that if \$G\$ is simply connected and the action \${\textbackslash}{textbackslashGamma}{\textbackslash}textbackslashcurvearrowright G\$ has property (T), then any cocycle \$w:{\textbackslash}{textbackslashGamma}{\textbackslash}textbackslashtimes G{\textbackslash}textbackslashrightarrow{\textbackslash}{textbackslashLambda}\$ with values into a countable group \${\textbackslash}{textbackslashLambda}\$ is cohomologous to a homomorphism \${\textbackslash}textbackslashdelta:{\textbackslash}{textbackslashGamma}{\textbackslash}textbackslashrightarrow{\textbackslash}{textbackslashLambda}\$. As a consequence, we deduce that the action \${\textbackslash}{textbackslashGamma}{\textbackslash}textbackslashcurvearrowright G\$ is orbit equivalent superrigid: any free nonsingular action \${\textbackslash}{textbackslashLambda}{\textbackslash}textbackslashcurvearrowright Y\$ which is orbit equivalent to \${\textbackslash}{textbackslashGamma}{\textbackslash}textbackslashcurvearrowright G\$, is necessarily conjugate to an induction of \${\textbackslash}{textbackslashGamma}{\textbackslash}textbackslashcurvearrowright G\$.},
	journaltitle = {{arXiv}:1406.6628 [math]},
	author = {Ioana, Adrian},
	urldate = {2019-10-12},
	date = {2014-06},
}

