@article{trujillo_abstract_2016,
	title = {From abstract alpha-Ramsey theory to abstract ultra-Ramsey theory},
	url = {http://arxiv.org/abs/1601.03831},
	abstract = {We work within the framework of the Alpha-Theory introduced by Benci and Di Nasso. The Alpha-Theory postulates a few natural properties for an infinite "ideal" number \${\textbackslash}textbackslashalpha\$. The formulation provides an elementary axiomatics for the methods of abstract ultra-Ramsey theory. The main results are Theorem 10, Theorem 57, Theorem 67 and Theorem 73. Theorem 10 is an infinite-dimensional extension of the celebrated Ramsey's Theorem. We show that corollaries of this result include the Galvin-Pirky Theorem, the Silver Theorem and the \${\textbackslash}textbackslashvec\{{\textbackslash}textbackslashalpha\}\$-Ellentuck Theorem. We prove that, under the assumption of the \${\textbackslash}textbackslashmathfrak\{c\}ˆ\{+\}\$-enlarging property, the \${\textbackslash}textbackslashvec\{{\textbackslash}textbackslashalpha\}\$-Ellentuck Theorem is equivalent to the Ultra-Ellentuck Theorem of Todorcevic. Theorem 57 is an abstraction of Theorem 10 to the setting of triples \$({\textbackslash}textbackslashmathcal\{R\},{\textbackslash}textbackslashle,r)\$ where \${\textbackslash}textbackslashmathcal\{R\}{\textbackslash}textbackslashnot={\textbackslash}textbackslashemptyset\$, \${\textbackslash}textbackslashle\$ is a quasi-order on \${\textbackslash}textbackslashmathcal\{R\}\$ and \$r\$ is a function with domain \${\textbackslash}textbackslashmathbb\{N\}{\textbackslash}textbackslashtimes {\textbackslash}textbackslashmathcal\{R\}\$. We use Theorem 57 to develop the Abstract \${\textbackslash}textbackslashvec\{{\textbackslash}textbackslashalpha\}\$-Ellentuck Theorem, Theorem 67, and the Abstract Ultra-Ellentuck Theorem, Theorem 73, extending the Abstract Ellentuck Theorem along the same lines as the \${\textbackslash}textbackslashvec\{{\textbackslash}textbackslashalpha\}\$-Ellentuck Theorem and Ultra-Ellentuck Theorem extend the Ellentuck Theorem, respectively. We conclude with some examples illustrating the theory and an open question related to the local Ramsey theory developed by Di Prisco, Mijares and Nieto.},
	journaltitle = {{arXiv}:1601.03831 [math]},
	author = {Trujillo, Timothy},
	urldate = {2018-07-14},
	date = {2016-01},
	keywords = {Mathematics - Logic},
}

