@article{bowen_generic_2014,
	title = {Generic stationary measures and actions},
	url = {http://arxiv.org/abs/1405.2260},
	abstract = {Let \$G\$ be a countably infinite group, and let \${\textbackslash}textbackslashmu\$ be a generating probability measure on \$G\$. We study the space of \${\textbackslash}textbackslashmu\$-stationary Borel probability measures on a topological \$G\$ space, and in particular on \${ZˆG}\$, where \$Z\$ is any perfect Polish space. We also study the space of \${\textbackslash}textbackslashmu\$-stationary, measurable \$G\$-actions on a standard, nonatomic probability space. Equip the space of stationary measures with the weak* topology. When \${\textbackslash}textbackslashmu\$ has finite entropy, we show that a generic measure is an essentially free extension of the Poisson boundary of \$(G,{\textbackslash}textbackslashmu)\$. When \$Z\$ is compact, this implies that the simplex of \${\textbackslash}textbackslashmu\$-stationary measures on \${ZˆG}\$ is a Poulsen simplex. We show that this is also the case for the simplex of stationary measures on \${\textbackslash}textbackslash\{0,1{\textbackslash}textbackslash\}ˆG\$. We furthermore show that if the action of \$G\$ on its Poisson boundary is essentially free then a generic measure is isomorphic to the Poisson boundary. Next, we consider the space of stationary actions, equipped with a standard topology known as the weak topology. Here we show that when \$G\$ has property (T), the ergodic actions are meager. We also construct a group \$G\$ without property (T) such that the ergodic actions are not dense, for some \${\textbackslash}textbackslashmu\$. Finally, for a weaker topology on the set of actions, which we call the very weak topology, we show that a dynamical property (e.g., ergodicity) is topologically generic if and only if it is generic in the space of measures. There we also show a Glasner-King type 0-1 law stating that every dynamical property is either meager or residual.},
	journaltitle = {{arXiv}:1405.2260 [math]},
	author = {Bowen, Lewis and Hartman, Yair and Tamuz, Omer},
	urldate = {2016-02-13},
	date = {2014-05},
}

