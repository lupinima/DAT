@article{bergfalk_strong_2015,
	title = {Strong Homology, Derived Limits, and Set Theory},
	url = {http://arxiv.org/abs/1509.09267},
	abstract = {We consider the question of the additivity of strong homology. This entails isolating the set-theoretic content of the higher derived limits of an inverse system indexed by the functions from \${\textbackslash}textbackslashmathbb\{N\}\$ to \${\textbackslash}textbackslashmathbb\{N\}\$. We show that this system governs, at a certain level, the additivity of strong homology over sums of arbitrary cardinality. We show in addition that, under the Proper Forcing Axiom, strong homology is not additive, not even on closed subspaces of \${\textbackslash}textbackslashmathbb\{R\}ˆ4\$.},
	journaltitle = {{arXiv}:1509.09267 [math]},
