@article{mohammadi_matrix_2012,
	title = {Matrix coefficients, Counting and Primes for orbits of geometrically finite groups},
	url = {http://arxiv.org/abs/1208.4139},
	abstract = {Let G:={SO}(n,1)ˆ{\textbackslash}textbackslashcirc and {\textbackslash}{textbackslashGamma} be a geometrically finite Zariski dense subgroup with critical exponent delta bigger than (n-1)/2. Under a spectral gap hypothesis on Lˆ2({\textbackslash}{textbackslashGamma} {\textbackslash}textbackslash G), which is always satisfied for delta{\textbackslash}textgreater(n-1)/2 for n=2,3 and for delta{\textbackslash}textgreatern-2 for n{\textbackslash}textgreater= 4, we obtain an \{{\textbackslash}textbackslashit effective\} archimedean counting result for a discrete orbit of {\textbackslash}{textbackslashGamma} in a homogeneous space H{\textbackslash}{textbackslashG}, where H is the trivial group, an affine symmetric subgroup or a horospherical subgroup. More precisely, we show that for any effectively well-rounded family \{B\_T\} of compact subsets in H{\textbackslash}{textbackslashG}, there exists {\textbackslash}textbackslasheta{\textbackslash}textgreater0 such that \#[e]{\textbackslash}{textbackslashG}{\textbackslash}textbackslashcap B\_T=M(B\_T) +O(M(B\_T)ˆ\{1-{\textbackslash}textbackslasheta\}) for an explicit measure M on H{\textbackslash}{textbackslashG}, which depends on Gamma. We also apply affine sieve and describe the distribution of almost primes on orbits of {\textbackslash}{textbackslashGamma} in arithmetic settings. One of key ingredients in our approach is an effective asymptotic formula for the matrix coefficients of Lˆ2({\textbackslash}{textbackslashGamma} {\textbackslash}textbackslash G) that we prove by combining methods from spectral analysis, harmonic analysis and ergodic theory. An effective mixing for the Bowen-Margulis-Sullivan measure is also obtained as an application of our methods.},
	journaltitle = {{arXiv}:1208.4139 [math]},
	author = {Mohammadi, Amir and Oh, Hee},
	urldate = {2015-03-10},
	date = {2012-08},
	keywords = {Mathematics - Dynamical Systems, Mathematics - Representation Theory, Mathematics - Geometric Topology, Mathematics - Number Theory, Mathematics - Spectral Theory},
}

