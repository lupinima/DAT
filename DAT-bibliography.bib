@article{lupini_looking_2022,
	title = {(Looking For) The Heart of Abelian Polish Groups},
	url = {http://arxiv.org/abs/2202.13439},
	abstract = {We prove that the category \${\textbackslash}mathcal\{M\}\$ of abelian groups with a Polish cover introduced in collaboration with Bergfalk and Panagiotopoulos is the left heart of (the derived category of) the quasi-abelian category \${\textbackslash}mathcal\{A\}\$ of abelian Polish groups in the sense of Beilinson--Bernstein--Deligne and Schneiders. Thus, \${\textbackslash}mathcal\{M\}\$ is an abelian category containing \${\textbackslash}mathcal\{A\}\$ as a full subcategory such that the inclusion functor \${\textbackslash}mathcal\{A\}{\textbackslash}rightarrow {\textbackslash}mathcal\{M\}\$ is exact and finitely continuous. Furthermore, \${\textbackslash}mathcal\{M\}\$ is uniquely characterized up to equivalence by the following universal property: for every abelian category \${\textbackslash}mathcal\{B\}\$, a functor \${\textbackslash}mathcal\{A\}{\textbackslash}rightarrow {\textbackslash}mathcal\{B\}\$ is exact and finitely continuous if and only if it extends to an exact (and necessarily finitely continuous) functor \${\textbackslash}mathcal\{M\}{\textbackslash}rightarrow {\textbackslash}mathcal\{B\}\$. We provide similar descriptions of the left heart of the following categories: non-Archimedean abelian Polish groups; (non-Archimedean) Polish \$G\$-modules, for a given Polish group or Polish ring \$G\$; Fr{\textbackslash}'\{e\}chet spaces over \$K\$ for a given separable complete non-Archimedean valued field \$K\$.},
