@article{richter_strictly_2018,
	title = {A strictly commutative model for the cochain algebra of a space},
	url = {http://arxiv.org/abs/1801.01060},
	abstract = {The commutative differential graded algebra \$A\_\{{\textbackslash}textbackslashmathrm\{{PL}\}\}(X)\$ of polynomial forms on a simplicial set \$X\$ is a crucial tool in rational homotopy theory. In this note, we construct an integral version \$Aˆ\{{\textbackslash}textbackslashmathcal\{I\}\}(X)\$ of \$A\_\{{\textbackslash}textbackslashmathrm\{{PL}\}\}(X)\$. Our approach uses diagrams of chain complexes indexed by the category of finite sets and injections \${\textbackslash}textbackslashmathcal\{I\}\$ to model \$E\_\{{\textbackslash}textbackslashinfty\}\$ differential graded algebras by strictly commutative objects, called commutative \${\textbackslash}textbackslashmathcal\{I\}\$-dgas. We define a functor \$Aˆ\{{\textbackslash}textbackslashmathcal\{I\}\}\$ from simplicial sets to commutative \${\textbackslash}textbackslashmathcal\{I\}\$-dgas and show that it is a commutative lift of the usual cochain algebra functor. In particular, it gives rise to a new construction of the \$E\_\{{\textbackslash}textbackslashinfty\}\$ dga of cochains. The functor \$Aˆ\{{\textbackslash}textbackslashmathcal\{I\}\}\$ shares many properties of \$A\_\{{\textbackslash}textbackslashmathrm\{{PL}\}\}\$, and can be viewed as a generalization of \$A\_\{{\textbackslash}textbackslashmathrm\{{PL}\}\}\$ that works over arbitrary commutative ground rings. Working over the integers, a theorem by Mandell implies that \$Aˆ\{{\textbackslash}textbackslashmathcal\{I\}\}(X)\$ determines the homotopy type of \$X\$ when \$X\$ is a nilpotent space of finite type.},
	journaltitle = {{arXiv}:1801.01060 [math]},
	author = {Richter, Birgit and Sagave, Steffen},
	urldate = {2019-12-14},
	date = {2018-09},
}

