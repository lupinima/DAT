@article{barber_partition_2013-1,
	title = {Partition regularity in the rationals},
	volume = {120},
	issn = {00973165},
	url = {http://arxiv.org/abs/1212.6648},
	doi = {10.1016/j.jcta.2013.05.011},
	abstract = {A system of homogeneous linear equations with integer coefficients is partition regular if, whenever the natural numbers are finitely coloured, the system has a monochromatic solution. The Finite Sums theorem provided the first example of an infinite partition regular system of equations. Since then, other such systems of equations have been found, but each can be viewed as a modification of the Finite Sums theorem. We present here a new infinite partition regular system of equations that appears to arise in a genuinely different way. This is the first example of a partition regular system in which a variable occurs with unbounded coefficients. A modification of the system provides an example of a system that is partition regular over Q but not N, settling another open problem.},
	pages = {1590--1599},
	number = {7},
	journaltitle = {Journal of Combinatorial Theory, Series A},
	author = {Barber, Ben and Hindman, Neil and Leader, Imre},
	urldate = {2020-04-14},
	date = {2013-09},
	keywords = {Mathematics - Combinatorics, 05D10},
}

@article{de_universally_2008,
	title = {Universally Image Partition Regularity},
	rights = {Copyright (c)},
	issn = {1077-8926},
	url = {https://www.combinatorics.org/ojs/index.php/eljc/article/view/v15i1r141},
	doi = {10.37236/865},
	abstract = {Many of the classical results of Ramsey Theory, for example Schur's Theorem, van der Waerden's Theorem, Finite Sums Theorem, are naturally stated in terms of image partition regularity of matrices. Many characterizations are known of image partition regularity over {NN}\{{\textbackslash}{textbackslashBbb} N\} and other subsemigroups of (R,+)(R,+)(\{{\textbackslash}{textbackslashBbb} R\},+). In this paper we introduce a new notion which we call universally image partition regular matrices, which are in fact image partition regular over all semigroups and everywhere. We also prove that such matrices exist in abundance.},
	pages = {R141--R141},
	journaltitle = {The Electronic Journal of Combinatorics},
	author = {De, Dibyendu and Paul, Ram Krishna},
	urldate = {2020-04-14},
	date = {2008-11},
	langid = {english},
}

@article{hindman_image_1993,
	title = {Image Partition Regularity of Matrices},
	volume = {2},
	issn = {1469-2163, 0963-5483},
	url = {https://www.cambridge.org/core/journals/combinatorics-probability-and-computing/article/image-partition-regularity-of-matrices/024A80D4FF53127AA36144C6AB252A9A},
	doi = {10.1017/S0963548300000821},
	abstract = {Many of the classical results of Ramsey Theory, including those of Hilbert, Schur, and van der Waerden, are naturally stated as instances of the following problem: given a u × ν matrix A with rational entries, is it true, that whenever the set ℕ of positive integers is finitely coloured, there must exist some x∈ℕν such that all entries of Ax are the same colour? While the theorems cited are all consequences of Rado's theorem, the general problem had remained open. We provide here several solutions for the alternate problem, which asks that x∈ℕν. Based on this, we solve the general problem, giving various equivalent characterizations.},
	pages = {437--463},
	number = {4},
	journaltitle = {Combinatorics, Probability and Computing},
	author = {Hindman, Neil and Leader, Imre},
	urldate = {2020-04-14},
	date = {1993-12},
	langid = {english},
}

@book{hindman_open_2003-1,
	title = {Open Problems in Partition Regularity},
	abstract = {A finite or infinite matrix A with rational entries is called partition regular if whenever the natural numbers are finitely coloured there is a monochromatic vector x with Ax = 0. Many of the classical theorems of Ramsey Theory may naturally be interpreted as assertions that particular matrices are partition regular. While in the finite case partition regularity is well understood, very little is known in the infinite case. Our aim in this paper is to present some of the natural and appealing open problems in the area.},
	author = {Hindman, Neil and Leader, Imre and Strauss, Dona},
	date = {2003},
}

