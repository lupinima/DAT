@article{phillips_tracial_2012,
	title = {The tracial Rokhlin property is generic},
	url = {http://arxiv.org/abs/1209.3859},
	abstract = {We prove several results of the following general form: automorphisms of (or actions of \$\{{\textbackslash}textbackslashmathbb\{Z\}\}ˆd\$ on) certain kinds of simple separable unital C*-algebras \$A\$ which have a suitable version of the Rokhlin property are generic among all automorphisms (or actions), or in a suitable class of automorphisms. That is, the ones with the version of the Rokhlin property contain a dense \$G\_\{{\textbackslash}textbackslashdelta\}\$-subset of the set of all such automorphisms (or actions). Specifically, we prove the following. If \$A\$ is stable under tensoring with the Jiang-Su algebra \$Z,\$ and has tracial rank zero, then automorphisms with the tracial Rokhlin property are generic. If \$A\$ has tracial rank zero, or, more generally, \$A\$ is tracially approximately divisible together with a technical condition, then automorphisms with the tracial Rokhlin property are generic among the approximately inner automorphisms. If \$A\$ is stable under tensoring with the Cuntz algebra \$\{{\textbackslash}textbackslashmathcal\{O\}\}\_\{{\textbackslash}textbackslashinfty\}\$ or with a {UHF} algebra of infinite type, then actions of \$\{{\textbackslash}textbackslashmathbb\{Z\}\}ˆd\$ on \$A\$ with the Rokhlin property are generic among all actions of \$\{{\textbackslash}textbackslashmathbb\{Z\}\}ˆd.\$ We further give a related but more restricted result for actions of finite groups.},
	journaltitle = {{arXiv}:1209.3859 [math]},
	author = {Phillips, N. Christopher},
	urldate = {2014-04-09},
	date = {2012-09},
	keywords = {Mathematics - Operator Algebras, 46L},
}

