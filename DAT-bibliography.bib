@misc{willett_uct_2022,
	title = {The {UCT} for \$C{\textasciicircum}*\$-algebras with finite complexity},
	url = {http://arxiv.org/abs/2104.10766},
	doi = {10.48550/arXiv.2104.10766},
	abstract = {A \$C{\textasciicircum}*\$-algebra satisfies the Universal Coefficient Theorem ({UCT}) of Rosenberg and Schochet if it is equivalent in Kasparov's \${KK}\$-theory to a commutative \$C{\textasciicircum}*\$-algebra. This paper is motivated by the problem of establishing the range of validity of the {UCT}, and in particular, whether the {UCT} holds for all nuclear \$C{\textasciicircum}*\$-algebras. We introduce the idea of a \$C{\textasciicircum}*\$-algebra that "decomposes" over a class \${\textbackslash}mathcal\{C\}\$ of \$C{\textasciicircum}*\$-algebras. Roughly, this means that locally, there are approximately central elements that approximately cut the \$C{\textasciicircum}*\$-algebra into two \$C{\textasciicircum}*\$-subalgebras from \${\textbackslash}mathcal\{C\}\$ that have well-behaved intersection. We show that if a \$C{\textasciicircum}*\$-algebra decomposes over the class of nuclear, {UCT} \$C{\textasciicircum}*\$-algebras, then it satisfies the {UCT}. The argument is based on controlled \${KK}\$-theory, as introduced by the authors in earlier work. Nuclearity is used via Kasparov's Hilbert module version of Voiculescu's theorem, and Haagerup's theorem that nuclear \$C{\textasciicircum}*\$-algebras are amenable We say that a \$C{\textasciicircum}*\$-algebra has finite complexity if it is in the smallest class of \$C{\textasciicircum}*\$-algebras containing the finite-dimensional \$C{\textasciicircum}*\$-algebras, and closed under decomposability; our main result implies that all \$C{\textasciicircum}*\$-algebras in this class satisfy the {UCT}. The class of \$C{\textasciicircum}*\$-algebras with finite complexity is large, and comes with an ordinal-number invariant measuring the complexity level. We conjecture that a \$C{\textasciicircum}*\$-algebra of finite nuclear dimension and real rank zero has finite complexity; this (and several other related conjectures) would imply the {UCT} for all separable nuclear \$C{\textasciicircum}*\$-algebras. We also give new local formulations of the {UCT}, and some other necessary and sufficient conditions for the {UCT} to hold for all nuclear \$C{\textasciicircum}*\$-algebras.},
	number = {{arXiv}:2104.10766},
	publisher = {{arXiv}},
	author = {Willett, Rufus and Yu, Guoliang},
	urldate = {2022-11-22},
	date = {2022-05-11},
	eprinttype = {arxiv},
	eprint = {2104.10766 [math]},
	keywords = {Mathematics - K-Theory and Homology, Mathematics - Operator Algebras, 19K35, 46L80, 46L85},
	file = {arXiv Fulltext PDF:C\:\\Users\\Martino Lupini\\Zotero\\storage\\LW2NBC5N\\Willett and Yu - 2022 - The UCT for \$C^\$-algebras with finite complexity.pdf:application/pdf},
