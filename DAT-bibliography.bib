@article{gardella_classification_2014,
	title = {Classification theorems for circle actions on Kirchberg algebras, {II}},
	url = {http://arxiv.org/abs/1406.1208},
	abstract = {In the first part of this work, we studied circle actions with the Rokhlin property on Kirchberg algebras. Here, we define a continuous analog of the Rokhlin property, asking for a continuous path of unitaries instead of a sequence. We show that circle actions with the continuous Rokhlin property on Kirchberg algebras are classified by the \${KK}\$-equivalence class of their fixed point algebra, and in the presence of the {UCT}, by their equivariant \$K\$-theory. We moreover characterize the \$K\$-theoretical invariants that arise in this way. As an important technical result, we show that the continuous Rokhlin property implies the existence of a unital completely positive asymptotic homomorphism from the algebra to its fixed point subalgebra, which is moreover a left inverse for the canonical inclusion. As a consequence, we show that the {UCT} is preserved under formation of crossed products and passage to fixed point algebras by such actions. Using our classification results for Rokhlin actions of the circle, we show that for large classes of unital \$Cˆ*\$-algebras, including all Kirchberg algebras with finitely generated \$K\$-theory, the Rokhlin property is in fact equivalent to the continuous Rokhlin property. We show, by means of an example, that the two notions are not equivalent in general, even on Kirchberg algebras that satisfy the {UCT}.},
	journaltitle = {{arXiv}:1406.1208 [math]},
	author = {Gardella, Eusebio},
	urldate = {2014-12-11},
	date = {2014-06},
	keywords = {Mathematics - Operator Algebras, Mathematics - Dynamical Systems, Secondary 46L35, 19K35, 46L80, Primary 46L55},
}

