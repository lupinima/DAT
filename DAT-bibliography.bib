@misc{krupinski_locally_2023,
	title = {Locally compact models for approximate rings},
	url = {http://arxiv.org/abs/2203.05609},
	doi = {10.48550/arXiv.2203.05609},
	abstract = {By an approximate subring of a ring we mean an additively symmetric subset \$X\$ such that \$X{\textbackslash}cdot X {\textbackslash}cup (X +X)\$ is covered by finitely many additive translates of \$X\$. We prove that each approximate subring \$X\$ of a ring has a locally compact model, i.e. a ring homomorphism \$f {\textbackslash}colon {\textbackslash}langle X {\textbackslash}rangle {\textbackslash}to S\$ for some locally compact ring \$S\$ such that \$f[X]\$ is relatively compact in \$S\$ and there is a neighborhood \$U\$ of \$0\$ in \$S\$ with \$f{\textasciicircum}\{-1\}[U] {\textbackslash}subseteq 4X + X {\textbackslash}cdot 4X\$ (where \$4X:=X+X+X+X\$). This \$S\$ is obtained as the quotient of the ring \${\textbackslash}langle X {\textbackslash}rangle\$ interpreted in a sufficiently saturated model by its type-definable ring connected component. The above theorem can be seen as a general structural result about approximate subrings: every approximate subring \$X\$ can be recovered up to additive commensurability as the preimage by a locally compact model \$f {\textbackslash}colon {\textbackslash}langle X {\textbackslash}rangle {\textbackslash}to S\$ of any relatively compact neighborhood of \$0\$ in \$S\$. It also leads to more precise structural or even classification results. For example, we deduce that every [definable] approximate subring \$X\$ of a ring of positive characteristic is additively commensurable with a [definable] subring contained in \$4X + X {\textbackslash}cdot 4X\$. This implies that for any given \$K,L {\textbackslash}in {\textbackslash}mathbb\{N\}\$ there exists \$C(K,L)\$ such that every \$K\$-approximate subring \$X\$ (i.e. \$K\$ additive translates of \$X\$ cover \$X {\textbackslash}cdot X {\textbackslash}cup (X+X)\$) of a ring of positive characteristic \${\textbackslash}leq L\$ is additively \$C(K,L)\$-commensurable with a subring contained in \$4X + X {\textbackslash}cdot 4X\$. We also deduce a classification of finite approximate subrings of rings without zero divisors: for every \$K {\textbackslash}in {\textbackslash}mathbb\{N\}\$ there exists \$N(K) {\textbackslash}in {\textbackslash}mathbb\{N\}\$ such that for every finite \$K\$-approximate subring \$X\$ of a ring without zero divisors either \${\textbar}X{\textbar} {\textless}N(K)\$ or \$4X + X {\textbackslash}cdot 4X\$ is a subring which is additively \$K{\textasciicircum}\{11\}\$-commensurable with \$X\$.},
	number = {{arXiv}:2203.05609},
	publisher = {{arXiv}},
	author = {Krupiński, Krzysztof},
	urldate = {2023-06-08},
