@article{hogben_linear_2010,
	title = {A linear algebraic view of partition regular matrices},
	volume = {433},
	issn = {0024-3795},
	url = {http://www.sciencedirect.com/science/article/pii/S002437951000337X},
	doi = {10.1016/j.laa.2010.06.041},
	abstract = {Rado showed that a rational matrix is partition regular over N if and only if it satisfies the columns condition. We investigate linear algebraic properties of the columns condition, especially for oriented (vertex-arc) incidence matrices of directed graphs and for sign pattern matrices. It is established that the oriented incidence matrix of a directed graph Γ has the columns condition if and only if Γ is strongly connected, and in this case an algorithm is presented to find a partition of the columns of the oriented incidence matrix with the maximum number of cells. It is shown that a sign pattern matrix allows the columns condition if and only if each row is either all zeros or the row has both a + and −.},
	pages = {1809--1820},
